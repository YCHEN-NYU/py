    "# %pylab qt\n",
    "\n",
    "fig = plt.figure(figsize=(8,6))\n",
    "fig.patch.set_facecolor('white')\n",
    "\n",
    "x = np.linspace(0,1,100)\n",
    "w = 0.1\n",
    "y = x**2\n",
    "plt.plot(x, y, 'b.')\n",
    "\n",
    "plt.xlabel('x')\n",
    "plt.ylabel('y')\n",
    "plt.grid(True)\n",
    "plt.show()"
